{
    "name": "evrinoma/dashboard-bundle",
    "description": "DashBoard",
    "keywords": [
        "DashBoard"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Nikolaev",
            "email": "evrinoma@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "evrinoma/system-bundle": "dev-master",
        "doctrine/collections": "^1",
        "symfony/http-kernel": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Evrinoma\\DashBoardBundle\\": "src/",
            "Evrinoma\\DashBoardBundle\\Tests\\": "tests/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "friendsofphp/php-cs-fixer": "^3.12",
        "friendsofsymfony/rest-bundle": "^3",
        "nelmio/api-doc-bundle": "4.9.0",
        "symfony/yaml": "^5.0",
        "symfony/browser-kit": "^5.2",
        "symfony/validator": "5.2.*",
        "symfony/intl": "5.3.*",
        "symfony/dotenv": "5.2.*",
        "symfony/dependency-injection": "^5.4",
        "symfony/serializer": "^6.0",
        "phpunit/phpunit": "^9.5.2",
        "evrinoma/test-utils-bundle": "dev-master",
        "evrinoma/hooks-bundle": "dev-master"
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "pre-package-uninstall": [
        ]
    },
    "repositories": {
        "dto": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/DtoBundle.git"
        },
        "hooks": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/HooksBundle.git"
        },
        "system": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/SystemBundle.git"
        },
        "test-utils": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/TestUtilsBundle.git"
        },
        "utils": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/UtilsBundle.git"
        }
    }
}
